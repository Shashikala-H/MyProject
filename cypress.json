{
  "baseUrl": "https://shoeshoprunning.fittedrunning.com/",
  "ignoreTestFiles": ["*.js", "*.md"],
  "viewportHeight": 800,
  "viewportWidth": 1200,
  "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "overwrite": false,
            "html": false,
            "json": true
        }
  },


  "chromeWebSecurity": false,
  "env": {
    "host": "https://arunnersmind.com/shop/",
    "runnersMindFittedRunning": "https://arunnersmind.fittedrunning.com/login/"
  }
  
}
